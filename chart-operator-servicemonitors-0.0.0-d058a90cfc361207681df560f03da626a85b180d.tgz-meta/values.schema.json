{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "disabled": {
            "type": "boolean"
        },
        "operator": {
            "type": "object",
            "properties": {
                "metricRelabelings": {
                    "type": "object"
                },
                "name": {
                    "type": "string"
                },
                "relabelings": {
                    "type": "object"
                },
                "scrapeInterval": {
                    "type": "string"
                },
                "scrapeTimeout": {
                    "type": "string"
                },
                "selectors": {
                    "type": "object",
                    "properties": {
                        "app": {
                            "type": "string"
                        },
                        "app.kubernetes.io/name": {
                            "type": "string"
                        }
                    }
                },
                "targetPort": {
                    "type": "string"
                }
            }
        },
        "team": {
            "type": "string"
        }
    }
}
